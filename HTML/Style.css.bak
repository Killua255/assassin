nav {
width: 100%;
height: 100px;
background-color: #fff;
}

ul {
	margin-left:50px;
}

ul li {
	list-style:none;
	display:inline-block;
	float: left;
	line-height: 100px;
}

ul li a {
	display: block;
	text-decoration: none;
	font-size: 30px;
	color: #1e1e1e;
	padding: 0 70px;
}

ul li a:hover{
	color: blue;
}

